<configuration>
  <system.webServer>


    <rewrite>
      <rules>

<rule name="rule 1b" stopProcessing="true">
	<match url="^(.*)/$"  />
	<action type="Rewrite" url="//{R:1}"  />
</rule>
<rule name="rule 2b" stopProcessing="true">
	<match url="^"  />
	<action type="Rewrite" url="/index.php"  />
</rule>
<rule name="rule 3b">
	<match url=".*"  />
	<action type="Rewrite" url="/-"  />
</rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>
